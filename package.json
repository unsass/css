{
    "name": "@unsass/css",
    "version": "2.2.0",
    "description": "Sass mixins to manage CSS declarations with custom properties option.",
    "scripts": {
        "lint:css": "stylelint --fix \"**/*.scss\"",
        "lint": "npm-run-all --parallel lint:*",
        "test": "jest",
        "prepare": "husky install",
        "release": "semantic-release"
    },
    "engines": {
        "node": ">=16.0.0",
        "npm": ">=8.0.0"
    },
    "sass": "index.scss",
    "devDependencies": {
        "@commitlint/cli": "^17.3.0",
        "@commitlint/config-conventional": "^17.3.0",
        "@semantic-release/changelog": "^6.0.2",
        "@semantic-release/git": "^10.0.1",
        "glob": "^7.2.0",
        "husky": "^8.0.2",
        "jest": "^29.3.1",
        "npm-run-all": "^4.1.5",
        "sass-true": "^6.1.0",
        "semantic-release": "^19.0.5",
        "stylelint": "^14.16.0",
        "stylelint-config-unsass": "^1.0.1"
    },
    "dependencies": {
        "@unsass/utilities": "^1.3.1",
        "sass": "^1.56.1"
    },
    "keywords": [
        "sass",
        "declaration",
        "custom properties",
        "unsass",
        "front-end"
    ],
    "license": "MIT",
    "homepage": "https://github.com/unsass/css#readme",
    "bugs": {
        "url": "https://github.com/unsass/css/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/unsass/css.git"
    },
    "files": [
        "src/**/*",
        "*.scss",
        "CHANGELOG.md"
    ],
    "publishConfig": {
        "access": "public"
    }
}
